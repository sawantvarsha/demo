  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=2000000 ./node_modules/@angular/cli/bin/ng serve --port 4200",
    "ssl": "ng serve --port 4200 --ssl",
    "server": "nodemon .\\server.js",
    "server2": "nodemon --trace-warnings .\\serverV2.js",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "node --max_old_space_size=2000000 ./node_modules/@angular/cli/bin/ng build --base-href=/EuroconnectPortal/ --aot=true",
    "build-publish": "node --max_old_space_size=2000000 ./node_modules/@angular/cli/bin/ng build --base-href=/EuroconnectPortal/ --aot=true --buildOptimizer=true --output-path C://inetpub/wwwroot/EuroconnectPortal",
    "post-build": "purgecss --css dist/EuroconnectPortal/*.css --content dist/EuroconnectPortal/index.html dist/EuroconnectPortal/*.js --output dist/EuroconnectPortal",
    "build-stats": "ng build --stats-json --prod --base-href=/EuroconnectPortal/ --buildOptimizer=true",
    "build-dev": "ng build --source-map=false --configuration dev --base-href=/EuroconnectPortal/ --outputPath=dist_dev --buildOptimizer=true",
    "post-build-dev": "purgecss --css dist_dev/*.css --content dist_dev/index.html dist_dev/*.js --output dist_dev",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "aot": "node --max-old-space-size=15000000 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "watch": "ng build --watch --configuration development"
  },
